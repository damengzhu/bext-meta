    "dayjs": "^1.10.7",
    "express": "^4.17.2",
    "lodash-es": "^4.17.21",
    "meow": "^10.1.2",
    "open": "^8.4.0",
    "shelljs": "^0.8.5",
    "simple-git": "^3.1.1"
  }
